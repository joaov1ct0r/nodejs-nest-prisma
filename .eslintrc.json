{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "@rocketseat/eslint-config/node",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/no-explicit-any": "off",
    "@typescript-eslint/semi": ["off"],
    "semi": "off",
    "no-useless-constructor": "off",
    "camelCase": "off"
  }
}
